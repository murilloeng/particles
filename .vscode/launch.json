{
	"version": "0.2.0",
	"configurations":
	[
		{
			"name": "gdb",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/dist/debug/particles.out",
			"args": [],
			"cwd": "${workspaceFolder}",
			"stopAtEntry": false,
